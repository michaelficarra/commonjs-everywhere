    "esmangle": "~1.0.0",
    "esprima": "1.x.x",
    "estraverse": "1.5.x",
    "events-browserify": "*",
    "http-browserify": "*",
    "mktemp": "0.3.x",
    "nopt": "~2.1.2",
    "punycode": "*",
    "querystring": "*",
    "resolve": "0.6.x",
    "vm-browserify": "*"
  },
  "devDependencies": {
    "coffee-script-redux": "2.0.0-beta8",
    "mocha": "~1.14.0",
    "scopedfs": "~0.1.0",
    "semver": "~2.2.1"
  },
  "scripts": {
    "test": "make test"
